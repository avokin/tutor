<!DOCTYPE html>
<html>
<head>
  <title>Tutor - <%=@title%></title>

  <%= stylesheet_link_tag "application" %>
  <%= javascript_include_tag "application" %>


  <%= csrf_meta_tag %>
</head>
<body>
<div class="container">
  <%= render 'layouts/header' %>
  <section class="round">
    <% flash.each do |key, value| %>
        <div class="flash <%= key %>"><%= value %></div>
    <% end %>
    <%= yield %>
  </section>
  <%= render 'layouts/footer' %>
</div>
</body>
</html>
