<!DOCTYPE html>
<html>
<head>
  <title>Tutor - <%=@title%></title>
  <%= stylesheet_link_tag 'application.css', 'jquery-ui-1.8.14.custom', 'tutor', 'jquery.fancybox-1.3.4' %>


  <%= javascript_include_tag :defaults %>

  <%= javascript_include_tag 'jquery-ui-1.8.14.custom.min', 'autocomplete-rails', 'tutor', 'jquery.fancybox-1.3.4.pack', 'jquery.mousewheel-3.0.4.pack' %>

  <script type="text/javascript">
      document.onkeypress = documentKeypress;

      $(document).ready(function() {
          $("a#shortkeys").fancybox();
      })
  </script>

  <%= csrf_meta_tag %>
</head>
<body>
<div class="container">
  <%= render 'layouts/header' %>
  <section class="round">
    <% flash.each do |key, value| %>
        <div class="flash <%= key %>"><%= value %></div>
    <% end %>
    <%= yield %>
  </section>
  <%= render 'layouts/footer' %>
</div>
</body>
</html>
